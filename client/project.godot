; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=3

[application]

config/name="spaceknights"
run/main_scene="res://lobby/lobby.tscn"
config/icon="res://icon.png"

[autoload]

djb2="*res://game/script/djb2/djb2.gd"
scalar="*res://game/script/fixed/scalar.gd"
vector="*res://game/script/fixed/vector.gd"
tcp="*res://game/script/tcp.gd"
stat="*res://game/script/stat.gd"
resource="*res://game/script/resource.gd"
user="*res://user.gd"

[display]

window/size/width=1080
window/size/height=1920
window/size/test_width=450
window/size/test_height=800
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep_height"

[rendering]

environment/default_environment="res://default_env.tres"
