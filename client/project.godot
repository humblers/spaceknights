; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=3

[application]

config/name="spaceknights"
run/main_scene="res://lobby.tscn"
config/icon="res://spaceknights_icon.png"

[autoload]

debug="*res://debug.gd"
global="*res://global.gd"
http_lobby="*res://http_lobby.gd"
input="*res://input.gd"
resource="*res://resource.gd"
tcp="*res://tcp.gd"

[display]

window/size/width=1080
window/size/height=1920
window/size/test_width=720
window/size/test_height=1280
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep_height"
window/stretch/shrink="1"

[image_loader]

filter=false
gen_mipmaps=false
