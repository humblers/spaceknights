script/source = "extends Node2D

onready var timer = Timer.new()

func _ready():
	timer.set_one_shot(true)
	timer.connect(\"timeout\", self, \"hide_bubble\")
	timer.set_wait_time(0.9)
	add_child(timer)

func show_bubble():
	show()
	timer.start()

func hide_bubble():
	hide()"

