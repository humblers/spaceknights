script/source = "extends Node

func _ready():
	print(OS.get_user_data_dir())
	$connect.connect(\"pressed\", self, \"connect_to_game_server\")
	$replay.connect(\"pressed\", self, \"replay\")

func load_game():
	tcp.Send({\"Id\": $id.text, \"Token\": $id.text})
	tcp.Send({\"GameId\": \"BEEF\"})
	scene_switcher.change_scene(\"res://test/nav.tscn\", { \"replay\": false })

func connect_to_game_server():
	tcp.Connect(\"127.0.0.1\", 9999)
	tcp.connect(\"connected\", self, \"load_game\")

func replay():
	scene_switcher.change_scene(\"res://test/nav.tscn\", { \"replay\": true })
"

